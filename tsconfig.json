{
    "compilerOptions": {
        "target": "ESNext",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "jsx": "react",
        "lib": ["dom", "esnext"],
        "moduleResolution": "node",
        "noImplicitAny": false,
        "skipLibCheck": true,
        "noEmit": true,
        "resolveJsonModule": true
    },
    "include": ["./src/**/*"],
    "exclude": ["node_modules"],
    "files": ["./globals.d.ts"]
}
